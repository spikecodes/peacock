{
"update_url": "https://clients2.google.com/service/update2/crx",

  
  "background": {
    "page": "Grammarly.html"
  },
  "browser_action": {
    "default_icon": "src/icon/icon48-chrome.png",
    "default_popup": "src/popup.html",
    "default_title": "Grammarly"
  },
  "content_scripts": [
    {
      "all_frames": false,
      "css": [
  "src/css/Grammarly.styles.css",
  "src/css/Grammarly-popup.styles.css"
],
      "js": [
  "src/js/Grammarly.js",
  "src/js/Grammarly.styles.js"
],
      "matches": [
        "<all_urls>"
      ],
      "run_at": "document_start"
    }
  ],
  "content_security_policy": "script-src 'self'; object-src 'self'",
  "description": "Enhance your written communication everywhere you type!",
  "externally_connectable": {
    "matches": [
      "*://*.grammarly.com/*"
    ]
  },
  "icons": {
    "128": "src/icon/icon128.png",
    "16": "src/icon/icon16.png",
    "48": "src/icon/icon48-chrome.png"
  },
  
  
  "manifest_version": 2,
  "name": "Grammarly for Chrome",
  "optional_permissions": [
    "clipboardRead"
  ],
  "permissions": [
    "<all_urls>",
    "tabs",
    "notifications",
    "cookies",
    "system.cpu",
    "clipboardWrite",
    
    "storage"
  ],
  "version": "14.930.5232",
  "web_accessible_resources": [ "src/signin.html", "src/popupEditorIFrame.html"  ]
}

